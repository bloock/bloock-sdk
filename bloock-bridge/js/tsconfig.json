{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "module": "es2020",
    "target": "es2020",
    "moduleResolution": "node",
    "lib": ["DOM", "ES2018"],
    "noEmit": true,
    "outDir": "dist",
    "rootDir": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": ["src/**/*", "test/**/*"]
}
